    "@typescript-eslint/parser": "^2.19.0",
    "codecov": "^3.6.5",
    "dts-bundle-generator": "^3.3.1",
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "jest": "^25.1.0",
    "rollup": "^1.31.0",
